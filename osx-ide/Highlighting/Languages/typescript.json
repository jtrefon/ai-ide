{
  "schemaVersion": 1,
  "language": "typescript",
  "highlighting": {
    "keywords": [
      "break", "case", "catch", "class", "const", "continue", "debugger", "default", "delete",
      "do", "else", "export", "extends", "finally", "for", "function", "if", "import", "in",
      "instanceof", "let", "new", "return", "super", "switch", "this", "throw", "try",
      "typeof", "var", "void", "while", "with", "yield", "async", "await",
      "interface", "type", "implements", "namespace", "abstract", "public", "private", "protected", "readonly"
    ],
    "typeKeywords": ["string", "number", "boolean", "any", "unknown", "never", "void"],
    "booleanLiterals": ["true", "false"],
    "nullLiterals": ["null", "undefined"]
  },
  "styling": {
    "tokenColors": {
      "keyword": "#569CD6",
      "type": "#4EC9B0",
      "boolean": "#D19A66",
      "null": "#D19A66",
      "number": "#D19A66",
      "string": "#CE9178",
      "comment": "#6A9955"
    },
    "fontTraitsByRole": {
      "keyword": "semibold",
      "type": "semibold",
      "comment": "italic"
    },
    "preferredFontFamily": null
  },
  "formatting": {
    "indentUnitStyle": "spaces",
    "indentWidth": 2,
    "trimTrailingWhitespace": true,
    "ensureTrailingNewline": true,
    "maxConsecutiveBlankLines": 1
  },
  "linting": {
    "registry": "builtin.typescript",
    "rules": [
      {
        "id": "no-explicit-any",
        "severity": "warning",
        "enabled": true,
        "message": "Avoid explicit any where possible",
        "options": {}
      },
      {
        "id": "consistent-type-imports",
        "severity": "warning",
        "enabled": true,
        "message": "Use type-only imports when importing types",
        "options": {
          "prefer": "type-imports"
        }
      }
    ]
  }
}
